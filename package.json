{
  "name": "frosty-native",
  "version": "1.0.0",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index",
  "exports": {
    ".": {
      "types": "./dist/index.mjs",
      "default": "./dist/index"
    },
    "./*": {
      "types": "./dist/*.mjs",
      "default": "./dist/*"
    }
  },
  "files": [
    "dist/*",
    "scripts/*",
    "packages/*",
    "template/*",
    "Package.swift"
  ],
  "repository": "https://github.com/o2ter/frosty-native.git",
  "author": "Susan Cheng <susan.doggie@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "rollup": "yarn clean && rollup -c",
    "test": "jest",
    "start": "npx frosty run -d -w -s ./src tests/server/app.tsx"
  },
  "bin": {
    "frosty-native": "./bin/frosty-native.sh"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@o2ter/colors.js": "^1.0.2",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "core-js": "^3.41.0",
    "file-loader": "^6.2.0",
    "frosty": "^0.0.142",
    "lodash": "^4.17.23",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.16",
    "dotenv-webpack": "^8.1.1",
    "jest": "^30.1.1",
    "rollup": "^4.22.4",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.4.1"
  }
}
